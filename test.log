w_handler = lg.StreamHandler()  
w_handler.setLevel(lg.WARNING)
e_handler = lg.FileHandler('file.log')  
w_handler.setLevel(lg.WARNING)  
e_handler.setLevel(lg.ERROR) 
logger_obj = lg.getLogger(__name__)
c_format = lg.Formatter('%(name)s - %(levelname)s - %(message)s')  
f_format = lg.Formatter('%(asctime)s - %(name)s - %(levelname)s - %(message)s')  
w_handler.setFormatter(c_format)  
e_handler.setFormatter(f_format)  
logger_obj.addHandler(w_handler)  
logger_obj.addHandler(e_handler)   